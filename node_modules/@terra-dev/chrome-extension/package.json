  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "typings": "index.d.ts",
  "name": "@terra-dev/chrome-extension",
  "version": "2.3.0",
  "dependencies": {
    "@terra-dev/browser-check": "^2.3.0",
    "@terra-dev/wallet-types": "^2.3.0",
    "@terra-money/terra.js": "^2.0.0",
    "rxjs": "^7.3.0"
  },
  "main": "./_commonjs/index.js",
  "module": "./index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./_commonjs/index.js",
      "node": "./_commonjs/index.js",
      "default": "./index.js"
    },
    "./public/*": "./public/*",
    "./ChromeExtensionController": {
      "import": "./ChromeExtensionController.js",
      "require": "./_commonjs/ChromeExtensionController.js",
      "node": "./_commonjs/ChromeExtensionController.js",
      "default": "./ChromeExtensionController.js"
    },
    "./ChromeExtensionController.js": {
      "import": "./ChromeExtensionController.js",
      "require": "./_commonjs/ChromeExtensionController.js",
      "node": "./_commonjs/ChromeExtensionController.js",
      "default": "./ChromeExtensionController.js"
    },
    "./errors": {
      "import": "./errors.js",
      "require": "./_commonjs/errors.js",
      "node": "./_commonjs/errors.js",
      "default": "./errors.js"
    },
    "./errors.js": {
      "import": "./errors.js",
      "require": "./_commonjs/errors.js",
      "node": "./_commonjs/errors.js",
      "default": "./errors.js"
    },
    "./extensionFixer": {
      "import": "./extensionFixer.js",
      "require": "./_commonjs/extensionFixer.js",
      "node": "./_commonjs/extensionFixer.js",
      "default": "./extensionFixer.js"
    },
    "./extensionFixer.js": {
      "import": "./extensionFixer.js",
      "require": "./_commonjs/extensionFixer.js",
      "node": "./_commonjs/extensionFixer.js",
      "default": "./extensionFixer.js"
    },
    "./README.md": "./README.md",
    "./storage": {
      "import": "./storage.js",
      "require": "./_commonjs/storage.js",
      "node": "./_commonjs/storage.js",
      "default": "./storage.js"
    },
    "./storage.js": {
      "import": "./storage.js",
      "require": "./_commonjs/storage.js",
      "node": "./_commonjs/storage.js",
      "default": "./storage.js"
    },
    "./types": {
      "import": "./types.js",
      "require": "./_commonjs/types.js",
      "node": "./_commonjs/types.js",
      "default": "./types.js"
    },
    "./types.js": {
      "import": "./types.js",
      "require": "./_commonjs/types.js",
      "node": "./_commonjs/types.js",
      "default": "./types.js"
    }
  }
}
